# Appwrite Configuration
# Copy this file to .env and fill in your actual values

# Appwrite Cloud Endpoint (or your self-hosted endpoint)
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1

# Your Appwrite Project ID (get this from Appwrite console)
VITE_APPWRITE_PROJECT_ID=your_project_id_here

# Database ID (will be created by setup script)
VITE_APPWRITE_DATABASE_ID=hexi-database

# Collections (will be created by setup script)
VITE_APPWRITE_USERS_COLLECTION_ID=users
VITE_APPWRITE_GROUPS_COLLECTION_ID=groups
VITE_APPWRITE_GROUPSCORES_COLLECTION_ID=group-scores
